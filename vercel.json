{
	"version":2,
	"builds": [
		{
			"src":"collage/wsgi.py",
			"use":"@vercel/python",
			"config":{"maxLamdaSize": "15mb","runtime": "python3.8.10"}

		},
		{
			"src":"build_files.sh",
			"use":"@vercel/static-build",
			"config":{
				"distDir": "staticfiles_build"
			}

		}
	],
	"routes":[
		{
			"src":"/static/(.*)",
			"dest":"static/$1"
		},
		{
			"src":"/(.*)",
			"dest":"collage/wsgi.py"
		}	
	]

}